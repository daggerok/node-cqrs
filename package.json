{
  "name": "node-cqrs",
  "version": "0.0.1",
  "description": "node CQRS eventsourcing rabbitmq mongodb",
  "main": "./messanger/index.js",
  "author": "daggerok",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.2",
    "express": "4.15.5",
    "mongodb": "2.2.31",
    "mongoose": "4.11.13",
    "morgan": "1.8.2",
    "nconf": "0.8.4",
    "servicebus": "2.0.10",
    "servicebus-register-handlers": "0.1.7",
    "servicebus-retry": "0.0.11",
    "servicebus-trace": "0.0.1"
  },
  "devDependencies": {
    "@types/express": "4.0.37",
    "@types/node": "8.0.31",
    "cross-env": "5.0.5",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1"
  },
  "scripts": {
    "composeUp": "docker-compose -f docker/docker-compose.yml up -d --build",
    "start": "npm-run-all -p start:*",
    "start:messanger": "cross-env MESSANGER_PORT=3000 nodemon ./messanger -w . -i node_modules",
    "composeDown": "docker-compose -f docker/docker-compose.yml down -v"
  }
}
